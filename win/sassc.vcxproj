    </ProjectConfiguration>
    <ProjectConfiguration Include="Release Optimize|x64">
      <Configuration>Release Optimize</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Release Training|Win32">
